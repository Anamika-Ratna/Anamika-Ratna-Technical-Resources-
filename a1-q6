import java.util.*;
public class Main{  
    
    public static void main(String[] args){
      // write your code here  
      System.out.println("enter the no. whose reverse you want to find");
      Scanner scn;
      scn=new Scanner(System.in);
      int r;
      int b=scn.nextInt();
      for (; b>0; b=b/10)
      {
        r=b%10;
        System.out.print(r);
      }
    }
  }
